GImage human_image;
GImage human_image_haft;

GButton btn;  // ปุ่มกด สำหรับ ส่งคำสั่งดึงรูปจาก Thai ID Reader
  bool request_image;
  uint8_t CMD_REQUEST_IMAGE[2] = { 0x31, 0x0D };


void image_show(GImage &image, uint8_t* data, size_t len, float zoom=100 /*10-100*/){
  File file = SPIFFS.open("/image.jpg", FILE_WRITE);
  if(!file){
    Serial.println("[File] error : can't open file for writing");
  }else{
    file.write( data, len);
    file.close();

    image = "SPIFFS://image.jpg";    
    image.zoom(zoom);
  }
}

void setup() {
  Serial.begin(115200); Serial.println();
  ThaiID_Reader.begin(9600,SERIAL_8N1, SERIAL2_RX, SERIAL2_TX);
  
  BlynkGO.begin();
  BlynkGO.fillScreen(TFT_RED, TFT_ORANGE);

  // นำข้อมูลรูป image_data ไปแสดงที่วิตเจ็ต GImage "human_image"  แสดงรูปแบบ 100%
  image_show( human_image, image_data, sizeof(image_data));
  human_image.align(ALIGN_LEFT, 40);
  
  // นำข้อมูลรูป image_data ไปแสดงที่วิตเจ็ต GImage "human_image_haft" โดยย่อรูป 50%
  image_show( human_image_haft, image_data, sizeof(image_data), 50);
  human_image_haft.align(ALIGN_RIGHT, -40); // ระยะปรับ align อาจะมีการเคลื่อนไป เนืองจากมีการ zoom


  btn = "ดึงรูปมาแสดง";
  btn.align(ALIGN_TOP_RIGHT,-20,20);
  btn.onClicked([](GWidget*widget){
    request_image = true;
    ThaiID_Reader.write(CMD_REQUEST_IMAGE, sizeof(CMD_REQUEST_IMAGE));
  });
}

void loop() {
  BlynkGO.update();

  if(request_image == true){
    if(ThaiID_Reader.available()){
      request_image = false;

      uint8_t img_data[5120];
      memset(img_data, 0, sizeof(img_data));
      if( ThaiID_Reader.read(img_data, sizeof(img_data)) ){
        // นำข้อมูลรูป img_data ไปแสดงที่วิตเจ็ต GImage "human_image"
        image_show( human_image, img_data, sizeof(img_data));
        human_image.align(ALIGN_LEFT, 40);
      }
    }
  }
}
